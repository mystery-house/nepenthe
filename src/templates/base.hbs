{{! This is the base template used by Nepenthe }}
{{! to construct Lilypond Documents.           }}
\version "{{lilypond_version}}"

\header {
    {{#if dedication}}
        dedication = "{{dedication}}"
    {{/if}}
    {{#if title}}
        title = "{{title}}"
    {{/if}}
    {{#if subtitle}}
        subtitle = "{{subtitle}}"
    {{/if}}
    {{#if subsubtitle}}
        subsubtitle = "{{subsubtitle}}"
    {{/if}}
    {{#if instrument}}
        instrument = "{{instrument}}"
    {{/if}}
    {{#if poet}}
        poet = "{{poet}}"
    {{/if}}
    {{#if meter}}
        meter = "{{meter}}"
    {{/if}}    
    {{#if composer}}
        composer = "{{composer}}"
    {{/if}}    
    {{#if arranger}}
        arranger = "{{arranger}}"
    {{/if}}
    {{#if tagline}}
        tagline = "{{tagline}}"
    {{/if}}
    {{#if copyright}}
        copyright = "{{copyright}}"
    {{/if}}
}

{{#each parts}}
{{>partPartial this}}
{{/each}}


{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "lib": ["es6", "es2015"],
        "types": ["node", "jest", "handlebars", "yaml-front-matter"],
        "declaration": true,
        "outDir": "../lib",
        "rootDir": "./",
        "strict": false,
        "noImplicitThis": false,
        "sourceMap": true
